((version "0.1.0")
 (synopsis "Convert nice looking MD to nice looking HTML")
 (author "siiky")
 (license "unlicense")
 (dependencies
  lowdown
  optimism
  srfi-1
  srfi-13
  sxml-transforms
  typed-records)
 (components
  (extension md2html
   (csc-options "-O0" "-d3")
   (modules md2html)
   (source md2html.scm))
  (program ssg
   (csc-options "-O0" "-d3")
   (component-dependencies md2html)
   (source ssg.scm))))
